<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.loan555.kidsapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:cantSaveState="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KidsApplication"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.MainActivityJava"></activity>
        <activity
            android:name=".ui.login.TaoTaiKhoanTreEmActivity"
            android:label="Tạo tài khoản trẻ em"
            android:parentActivityName=".ui.main.MainActivity" />
        <activity
            android:name=".ui.login.RegisterActivity"
            android:label="Tạo tài khoản người dùng"
            android:parentActivityName=".ui.login.LoginActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="Đăng nhập"
            android:parentActivityName=".ui.main.MainActivity" />
        <activity
            android:name=".ui.detailnew.DetailActivity"
            android:label=""
            android:parentActivityName=".ui.main.MainActivity" />
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>